{
    "language": "en_US",
    "ignoreWordsList": [
        "Cheatsheet",
        "Chromebook",
        "ReaspberryPi",
        "plugins",
        "blog",
        "GitHub",
        "Metadata",
        "Arduinos",
        "MagPi",
        "RaspberryPi"
    ],
    "documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "ignoreRegExp": [
        "/\\\\(.*\\\\.(jpg|jpeg|png|md|gif|JPG|JPEG|PNG|MD|GIF)\\\\)/g",
        "/((http|https|ftp|git)\\\\S*)/g",
        "/^(```\\\\s*)(\\\\w+)?(\\\\s*[\\\\w\\\\W]+?\\\\n*)(```\\\\s*)\\\\n*$/gm"
    ],
    "ignoreFileExtensions": [
        ".env"
    ],
    "checkInterval": 5000
}